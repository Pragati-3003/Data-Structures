<!-- https://leetcode.com/problems/first-missing-positive/description/ -->

class Solution {
public:
    int firstMissingPositive(vector<int>& nums) {
        int j = 1;

        sort(nums.begin(), nums.end());
        
        for (int i = 0; i < nums.size(); i++) {
            if (nums[i] < j) {
                continue;
            }

            if (nums[i] == j) {
                j++;
            }
        }

        return j;
    }
};